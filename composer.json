{
    "name": "pjmazenot/metamorphose",
    "description": "Metamorphose",
    "keywords": ["tool"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.3.0"
    },
    "require-dev": {
        "codeception/codeception": "^3.1",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Metamorphose\\": "src/"
        }
    },
    "autoload-dev": {
